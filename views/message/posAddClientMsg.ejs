<% if( errors2 ){ %>
    <% errors2.forEach((err)=>{ %>
        <div class="alert alert-danger">
            <%= err.msg %>
        </div>        
    <%});%>
<%}%>



<% if( (message) && (message != "") ){ %>
    <div class="alert alert-success">
        <%= message %>
    </div>
<%};%>



<% if( (error) && (error != "") ){ %>
    <div class="alert alert-danger">
        <%= error %>
    </div>
<%};%>